+/* pthread_spin_lock -- lock a spin lock. Generic version.
+   Copyright (C) 2012-2019 Free Software Foundation, Inc.
+   This file is part of the GNU C Library.
+   The GNU C Library is free software; you can redistribute it and/or
+   modify it under the terms of the GNU Lesser General Public
+   License as published by the Free Software Foundation; either
+   version 2.1 of the License, or (at your option) any later version.
+   The GNU C Library is distributed in the hope that it will be useful,
+   but WITHOUT ANY WARRANTY; without even the implied warranty of
+   MERCHANTABILITY or FITNESS FOR A PARTICULAR PURPOSE.  See the GNU
+   Lesser General Public License for more details.
+   You should have received a copy of the GNU Lesser General Public
+   License along with the GNU C Library.  If not, see
+   <http://www.gnu.org/licenses/>.  */
+
+#include "port_aarch64.h"
+
+/* Assumptions:
+ *
+ * ARMv8-a, AArch64
+ */
+
+ENTRY (pthread_spin_lock)
+        DELOUSE (0)
+
+        mov     w1, #0x1  //must be unchanged
+
+L(spin):
+		prfm    pldl1strm, [x0]
+		add		w3, w3, #0x1
+        ldaxr   w2, [x0]
+        cbnz    w2, L(spin)
+        stxr    w2, w1, [x0]
+        cbnz    w2, 1f
+		b		L(end)
+
+8:
+		wfe  //wait 50 us
+		b		L(normal)
+7:
+		mov		w6, #0x1b58  //loop times
+		b       L(loop_fun)
+
+6:
+		mov		w6, #0x1770  //loop times
+		b       L(loop_fun)
+
+5:
+		mov     w6, #0x1388  //loop times
+		b       L(loop_fun)
+
+4:		mov 	w6, #0x0fa0  //loop times
+		b       L(loop_fun)
+
+3:
+		mov     w6, #0x0bb8  //loop times
+		b       L(loop_fun)
+
+2:		mov     w6, #0x07d0  //loop times
+		b       L(loop_fun)
+   
+1:
+		mov     w6, #0x03e8  //loop times
+		b		L(loop_fun)
+
+L(normal):
+		ldr		w2, [x0]
+		cbz		w2, L(spin)	
+		and     w3, w3, #0x07  //get lower 3 bits
+        cmp     w3, #0x01
+        beq     1b
+        cmp     w3, #0x02
+        beq     2b
+        cmp     w3, #0x03
+        beq     3b
+		cmp     w3, #0x04
+        beq     4b
+		cmp     w3, #0x05
+        beq     5b
+		cmp     w3, #0x06
+        beq     6b
+		cmp     w3, #0x07
+        beq     7b
+		cmp     w3, #0x0
+        beq     8b
+        bne     L(spin)	
+
+L(loop_fun):
+		mov		w5, #0x0
+L(loop):
+		add		w5, w5, #0x1
+		cmp		w5, w6    //w6 times
+		b.lt	L(loop)
+		b		L(normal)		
+
+L(end):
+        mov     w0, #0x0
+        ret
+        nop
+
+END (pthread_spin_lock)
+libc_hidden_builtin_def (pthread_spin_lock)
+weak_alias (pthread_spin_lock, index)
